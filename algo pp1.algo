PROCEDURE Dot product (v1, v2 : ARRAY OF FLOAT, var ps:FLOAT  )                                 
VAR
    i : intger;
BEGIN
ps:=0
    FOR i FROM 0 TO v1.length  STEP 1  DO
            ps := ps + (v1[i] * v2[i]) ;
     END_IF
    END_FOR
    write(ps);
END
ALGORITHM  Dot product
VAR
   ps:FLOAT;
   v1,v2:ARRAY OF FLOAT[10]
BEGIN
   ps:= dot product (v1,v2,ps)
     IF ( ps=0) THEN
     write("The two vectors orthogonal")
     ELSE
     write("The two vectors not orthogonal")   
   END_IF
END